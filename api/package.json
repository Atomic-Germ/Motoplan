{
  "name": "motorcycle-trip-planner-api",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.ts' --fix",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=http.integration.test.ts --runInBand",
    "ingest:pois": "ts-node src/ingest/ingest-pois.ts",
    "ingest:roads": "ts-node src/ingest/ingest-roads.ts",
    "ingest:osm": "ts-node src/ingest/import-osm.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "typescript": "^5.5.6",
    "ts-node-dev": "^2.0.0",
    "@types/node": "^20.6.1",
    "@types/express": "^4.17.21",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.9.0",
    "prettier": "^3.0.0",
    "jest": "^29.6.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.4",
    "stream-json": "^1.7.5",
    "yargs": "^17.7.2",
    "@types/geojson": "^7946.0.8"
  }
}
